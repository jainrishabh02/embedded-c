#include <stdio.h>
#include <stdint.h>

// Function to multiply any number by 9 using bitwise operations
int32_t multiplyByNine(int32_t num) {
    return (num << 3) + num;  // Fast multiplication: (num * 8) + num
}

int main(void) {
    int32_t number = 7; // You can change this value for testing
    int32_t result = multiplyByNine(number);

    printf("Result of %d * 9 = %d\n", number, result);

    return 0;
}
